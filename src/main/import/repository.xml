<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <templates-ex-machina j:dependencies="default skins"
                            j:installedModules="templates-ex-machina"
                            j:modulePriority="0"
                            j:moduleType="templatesSet"
                            j:title="Templates Ex Machina"
                            jcr:mixinTypes="jmix:hasExternalProviderExtension"
                            jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder">
               <themes jcr:primaryType="jnt:folder">
                  <default jcr:primaryType="jnt:folder"/>
                  <blue jcr:primaryType="jnt:folder">
                     <blue.css jcr:primaryType="jnt:file">
                        <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                     </blue.css>
                  </blue>
                  <green jcr:primaryType="jnt:folder">
                     <green.css jcr:primaryType="jnt:file">
                        <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                     </green.css>
                  </green>
               </themes>
            </files>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <base j:theme="#/modules/templates-ex-machina/templates/files/themes/default"
                  j:view="templates-ex-machina"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
               <pagecontent jcr:primaryType="jnt:contentList">
                  <header j:id="header"
                          j:skin="skins.custom"
                          jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                          jcr:primaryType="jnt:area">
                     <header j:classname="container"
                             j:skin="skins.custom"
                             jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                             jcr:primaryType="jnt:contentList">
                        <logo-area j:id="logo"
                                   j:limitedAbsoluteAreaEdit="false"
                                   j:skin="skins.custom"
                                   jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                   jcr:primaryType="jnt:absoluteArea"/>
                        <navigation j:baselineNode="home"
                                    j:maxDepth="1"
                                    j:menuItemView="rollover-menu"
                                    j:startLevel="0"
                                    j:view="ex.machina"
                                    jcr:mixinTypes="jmix:renderable"
                                    jcr:primaryType="jnt:navMenu">
                           <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Navigation"/>
                        </navigation>
                     </header>
                  </header>
                  <rollover elementType="div"
                            j:allowedTypes="exmac:rolloverMenu"
                            j:classname="rollover"
                            j:id="rolloverid"
                            j:skin="skins.elementCustom"
                            jcr:mixinTypes="exmac:elementSkin jmix:skinnable"
                            jcr:primaryType="jnt:area"/>
                  <banner_area j:id="banner"
                               j:skin="skins.custom"
                               jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                               jcr:primaryType="jnt:area">
                     <banner_area j:classname="container"
                                  j:skin="skins.custom"
                                  jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                  jcr:primaryType="jnt:contentList">
                        <override-banner j:allowedTypes="exmac:bannerImage"
                                         j:limitedAbsoluteAreaEdit="false"
                                         j:numberOfItems="1"
                                         jcr:primaryType="jnt:absoluteArea"/>
                     </banner_area>
                  </banner_area>
                  <main-area j:id="page"
                             j:skin="skins.custom"
                             jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                             jcr:primaryType="jnt:area"/>
                  <featured j:id="featured"
                            j:skin="skins.custom"
                            jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                            jcr:primaryType="jnt:contentList">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Featured"/>
                     <container j:classname="container"
                                j:skin="skins.custom"
                                jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                jcr:primaryType="jnt:contentList">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Container"/>
                        <featured-row j:classname="row"
                                      j:skin="skins.custom"
                                      jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                      jcr:primaryType="jnt:contentList">
                           <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Featured Row"/>
                           <featured-1 j:limitedAbsoluteAreaEdit="false"
                                       j:skin="skins.section-4u"
                                       jcr:mixinTypes="jmix:skinnable"
                                       jcr:primaryType="jnt:absoluteArea"/>
                           <featured-2 j:limitedAbsoluteAreaEdit="false"
                                       j:skin="skins.section-4u"
                                       jcr:mixinTypes="jmix:skinnable"
                                       jcr:primaryType="jnt:absoluteArea"/>
                           <featured-3 j:limitedAbsoluteAreaEdit="false"
                                       j:skin="skins.section-4u"
                                       jcr:mixinTypes="jmix:skinnable"
                                       jcr:primaryType="jnt:absoluteArea"/>
                        </featured-row>
                     </container>
                  </featured>
                  <footer j:id="footer"
                          j:skin="skins.custom"
                          jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                          jcr:primaryType="jnt:contentList">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Footer"/>
                     <container j:classname="container"
                                j:skin="skins.custom"
                                jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                jcr:primaryType="jnt:contentList">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Container"/>
                        <row-footer-area j:classname="row"
                                         j:limitedAbsoluteAreaEdit="false"
                                         j:skin="skins.custom"
                                         jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                         jcr:primaryType="jnt:absoluteArea"/>
                     </container>
                  </footer>
               </pagecontent>
               <homepage jcr:primaryType="jnt:pageTemplate">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Home Page"/>
                  <main-area jcr:primaryType="jnt:contentList">
                     <marketing-container j:classname="container"
                                          j:id="marketing"
                                          j:skin="skins.custom"
                                          jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                          jcr:primaryType="jnt:contentList">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Marketing Container"/>
                        <row-marketing j:classname="row"
                                       j:skin="skins.custom"
                                       jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                       jcr:primaryType="jnt:contentList">
                           <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="row-marketing"/>
                           <mkt1 j:classname="3u"
                                 j:skin="skins.custom"
                                 jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                 jcr:primaryType="jnt:area"/>
                           <mkt2 j:classname="3u"
                                 j:skin="skins.custom"
                                 jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                 jcr:primaryType="jnt:area"/>
                           <mkt3 j:classname="3u"
                                 j:skin="skins.custom"
                                 jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                 jcr:primaryType="jnt:area"/>
                           <mkt4 j:classname="3u"
                                 j:skin="skins.custom"
                                 jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                 jcr:primaryType="jnt:area"/>
                        </row-marketing>
                     </marketing-container>
                     <home-main-area j:classname="container"
                                     j:id="main"
                                     j:skin="skins.custom"
                                     jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                     jcr:primaryType="jnt:contentList">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Home Main Area"/>
                        <row j:classname="row"
                             j:skin="skins.custom"
                             jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                             jcr:primaryType="jnt:contentList">
                           <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Row"/>
                           <left-6u j:classname="6u"
                                    j:skin="skins.custom"
                                    jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                    jcr:primaryType="jnt:area"/>
                           <center-3u j:classname="3u"
                                      j:skin="skins.custom"
                                      jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                      jcr:primaryType="jnt:area"/>
                           <right-3u j:classname="3u"
                                     j:skin="skins.custom"
                                     jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                     jcr:primaryType="jnt:area"/>
                        </row>
                     </home-main-area>
                  </main-area>
               </homepage>
               <main j:hiddenTemplate="false" jcr:primaryType="jnt:template">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Main"/>
                  <no-sidebar jcr:primaryType="jnt:pageTemplate">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="No Side Bar"/>
                     <row jcr:primaryType="jnt:contentList">
                        <_x0031_2u j:classname="12u"
                                   j:skin="skins.custom"
                                   jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                   jcr:primaryType="jnt:area"/>
                     </row>
                  </no-sidebar>
                  <main-area j:classname="container"
                             j:id="main"
                             j:skin="skins.custom"
                             jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                             jcr:primaryType="jnt:contentList">
                     <row j:classname="row"
                          j:skin="skins.custom"
                          jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                          jcr:primaryType="jnt:area"/>
                  </main-area>
                  <left-side-bar j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Left Side Bar"/>
                     <row jcr:primaryType="jnt:contentList">
                        <_x0033_u j:classname="3u"
                                  j:skin="skins.custom"
                                  jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                  jcr:primaryType="jnt:area"/>
                        <_x0039_u j:classname="9u"
                                  j:skin="skins.custom"
                                  jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                  jcr:primaryType="jnt:area"/>
                     </row>
                  </left-side-bar>
                  <right-side-bar j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Right Side Bar"/>
                     <row jcr:primaryType="jnt:contentList">
                        <_x0039_u j:classname="9u"
                                  j:skin="skins.custom"
                                  jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                  jcr:primaryType="jnt:area"/>
                        <_x0033_u j:classname="3u"
                                  j:skin="skins.custom"
                                  jcr:mixinTypes="jmix:customSkin jmix:skinnable"
                                  jcr:primaryType="jnt:area"/>
                     </row>
                  </right-side-bar>
               </main>
            </base>
            <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="templates-ex-machina"
                              jcr:primaryType="jnt:contentTemplate">
               <pagecontent jcr:primaryType="jnt:contentList">
                  <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
               </pagecontent>
            </content-template>
         </templates>
         <home j:isHomePage="true"
               j:templateName="homepage"
               jcr:primaryType="jnt:page">
            <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
         </home>
      </templates-ex-machina>
   </modules>
</content>